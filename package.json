{
  "name": "govuk-eleventy-plugin",
  "version": "1.0.5",
  "description": "Build websites using the GOV.UK design system and the Eleventy static site generator",
  "keywords": [
    "govuk",
    "government",
    "eleventy",
    "eleventy-plugin"
  ],
  "homepage": "https://govuk-eleventy.herokuapp.com",
  "bugs": {
    "url": "https://github.com/paulrobertlloyd/govuk-eleventy/issues"
  },
  "author": "Paul Robert Lloyd",
  "license": "MIT",
  "files": [
    "app",
    "lib"
  ],
  "main": "index.js",
  "sass": "app/all.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulrobertlloyd/govuk-eleventy.git"
  },
  "scripts": {
    "prebuild": "rimraf public",
    "build": "eleventy --quiet",
    "predev": "rimraf public",
    "dev": "eleventy --serve --quiet",
    "prestart": "npm run build",
    "start": "http-server",
    "lint": "standard && stylelint 'app/**/*.scss'",
    "test": "ava",
    "test:watch": "ava --watch",
    "coverage": "c8 ava"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "accessible-autocomplete": "^2.0.3",
    "govuk-frontend": "^4.0.0",
    "govuk-prototype-components": "x-govuk/govuk-prototype-components",
    "http-server": "^14.0.0",
    "luxon": "^2.1.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-govuk": "x-govuk/markdown-it-govuk",
    "markdown-it-image-figures": "^2.0.0",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "ava": "^4.0.1",
    "c8": "^7.11.0",
    "standard": "^16.0.4",
    "stylelint": "^13.13.1",
    "stylelint-config-gds": "^0.1.0"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "c8": {
    "reporter": [
      "text",
      "lcovonly"
    ]
  },
  "standard": {
    "ignore": [
      "!.eleventy.js"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-gds/scss"
  }
}
